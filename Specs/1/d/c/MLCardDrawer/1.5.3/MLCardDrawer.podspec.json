{
  "name": "MLCardDrawer",
  "version": "1.5.3",
  "summary": "MLCardDrawer for iOS",
  "homepage": "https://github.com/mercadolibre/meli-card-drawer-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Joni Bandoni",
  "source": {
    "git": "https://github.com/mercadolibre/meli-card-drawer-ios.git",
    "tag": "1.5.3"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "default_subspecs": "Default",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Default",
      "resources": [
        "Source/Assets/*.bundle"
      ],
      "source_files": [
        "Source/Classes/**/*.{h,m,swift}"
      ],
      "resource_bundles": {
        "MLCardDrawerResources": [
          "Source/Assets/*.xcassets",
          "Source/Classes/**/*.xib"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
