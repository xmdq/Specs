{
  "name": "DCFramworks",
  "version": "1.0.2",
  "summary": "DCFramworks",
  "description": "DCFramworks for DCITS SmartBank",
  "homepage": "https://gitee.com/wangyp330/dcframeworks",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "wangyp330": "wangypad@dcits.com"
  },
  "source": {
    "git": "https://gitee.com/wangyp330/dcframeworks.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "resource_bundles": {
    "DCFramworks": [
      "DCFramworks/Assets/*.bundle"
    ]
  },
  "frameworks": "UIKit",
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "subspecs": [
    {
      "name": "DCAvchatKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCAvchatKit.framework"
    },
    {
      "name": "DCAvchatSDK",
      "vendored_frameworks": "DCFramworks/Classes/*DCAvchatSDK.framework"
    },
    {
      "name": "DCAvchatUISDK",
      "vendored_frameworks": "DCFramworks/Classes/*DCAvchatUISDK.framework"
    },
    {
      "name": "DCFaceKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCFaceKit.framework"
    },
    {
      "name": "DCMessageKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCMessageKit.framework"
    },
    {
      "name": "DCOcrKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCOcrKit.framework"
    },
    {
      "name": "DCSecureKeyboardKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCSecureKeyboardKit.framework"
    },
    {
      "name": "DCUtilKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCUtilKit.framework"
    },
    {
      "name": "DCVoiceKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCVoiceKit.framework"
    },
    {
      "name": "DCWhiteBoardKit",
      "vendored_frameworks": "DCFramworks/Classes/*DCWhiteBoardKit.framework"
    }
  ]
}
