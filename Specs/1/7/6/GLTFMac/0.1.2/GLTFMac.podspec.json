{
  "name": "GLTFMac",
  "version": "0.1.2",
  "summary": "A library for loading glTF 2.0 assets into iOS runtime.",
  "homepage": "https://github.com/61315/gltf-mac",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Minseo Park": "studio61315@gmail.com"
  },
  "source": {
    "git": "https://github.com/61315/gltf-mac.git",
    "tag": "0.1.2"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source_files": [
    "GLTFMac/Classes/Headers/**/*",
    "GLTFMac/Classes/Source/**/*"
  ],
  "private_header_files": "GLTFMac/Classes/Headers/**/*.{h,hpp}",
  "public_header_files": "GLTFMac/Classes/Source/**/*.{h,hpp}",
  "vendored_libraries": "GLTFMac/Classes/Libraries/libdracodec.a",
  "header_mappings_dir": "GLTFMac",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/GLTFMac/Classes/Headers\"",
    "ONLY_ACTIVE_ARCH": "YES"
  },
  "resources": "GLTFMac/NOTICES",
  "requires_arc": true
}
