{
  "name": "ffmpeg-kit-tvos-https-gpl",
  "version": "4.4.LTS",
  "summary": "FFmpeg Kit tvOS Https GPL Static Framework",
  "description": "Includes FFmpeg with gmp, gnutls, libvid.stab, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.4.LTS/ffmpeg-kit-https-gpl-4.4.LTS-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "gmp.framework",
      "gnutls.framework",
      "libhogweed.framework",
      "libnettle.framework",
      "libvidstab.framework",
      "x264.framework",
      "x265.framework",
      "xvidcore.framework"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  }
}
