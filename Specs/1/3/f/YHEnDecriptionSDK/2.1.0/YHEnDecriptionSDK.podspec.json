{
  "name": "YHEnDecriptionSDK",
  "version": "2.1.0",
  "summary": "公司基础SDK之一",
  "description": "基础SDK之一，提供加密、解密、签名、验证签名。",
  "homepage": "https://github.com/XmYlzYhkj/YHEnDecriptionSDK",
  "license": "MIT",
  "authors": {
    "XmYhkj": "yhkj_xm@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/XmYlzYhkj/YHEnDecriptionSDK.git",
    "tag": "2.1.0"
  },
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-lstdc++"
  },
  "default_subspecs": "EncryptFull",
  "subspecs": [
    {
      "name": "EncryptFull",
      "vendored_frameworks": [
        "Frameworks/EncryptFull/YHEnDecriptionSDK.framework"
      ]
    },
    {
      "name": "EncryptAES",
      "vendored_frameworks": [
        "Frameworks/EncryptAES/YHEnDecriptionSDK.framework"
      ],
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "YHEnDecriptionExcludingSM=1"
      }
    }
  ]
}
