{
  "name": "Replay",
  "version": "0.0.1-beta1",
  "summary": "Replay: Record & Replay JSON webservices responses easily!",
  "description": "create and use JSON file to mock your app easily!\"\n\n2 modes available:\n- record\n- mock-responses\n\noverride json files on asset folder by following file nomenclature!",
  "homepage": "https://github.com/Axance-Technology-Open-Source/replay-ios",
  "license": {
    "type": "MIT",
    "file": "License.md"
  },
  "authors": {
    "Charles Legrand": "charles.legrand@devoteam.com"
  },
  "source": {
    "git": "https://github.com/Axance-Technology-Open-Source/replay-ios.git",
    "tag": "0.0.1-beta1"
  },
  "swift_versions": "5",
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "Replay/*.{swift}",
  "default_subspecs": "Moya14",
  "subspecs": [
    {
      "name": "Moya14",
      "dependencies": {
        "Moya/Core": [
          "~> 14.0"
        ]
      },
      "source_files": [
        "Replay/*.{swift}",
        "Replay/Moya/*.{swift}"
      ],
      "platforms": {
        "ios": "10.0"
      }
    },
    {
      "name": "Moya13",
      "dependencies": {
        "Moya/Core": [
          "~> 13.0"
        ]
      },
      "source_files": [
        "Replay/*.{swift}",
        "Replay/Moya/*.{swift}"
      ]
    },
    {
      "name": "Moya12",
      "dependencies": {
        "Moya/Core": [
          "~> 12.0"
        ]
      },
      "source_files": [
        "Replay/*.{swift}",
        "Replay/Moya/*.{swift}"
      ]
    }
  ],
  "swift_version": "5"
}
