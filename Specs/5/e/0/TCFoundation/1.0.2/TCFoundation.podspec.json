{
  "name": "TCFoundation",
  "version": "1.0.2",
  "summary": "iOS OC基础库Foundation",
  "description": "iOS OC基础库Foundation，目前支持：network、io、config、event等基础功能",
  "homepage": "https://github.com/flatads/tcfundation",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "flatads": "flatincbr.dev@gmail.com"
  },
  "source": {
    "git": "https://github.com/flatads/tcfundation.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "libraries": "c++",
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "Foundation",
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "MobileCoreServices"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "vendored_frameworks": "TCFoundation/**/*.{framework}",
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "FMDB": [
      "~> 2.6.2"
    ],
    "TCUtil": [
      "~> 1.0.2"
    ]
  }
}
