{
  "name": "TIMMessagesSDK",
  "version": "1.0.2",
  "summary": "TIMMessagesSDK for IOS",
  "description": "TIMMessagesSDK.framework一款用于即时视频聊天通讯的sdk",
  "homepage": "https://github.com/loneKiss/TIMMessagesSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "loneKiss": "775865767@qq.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/loneKiss/TIMMessagesSDK.git",
    "tag": "1.0.2"
  },
  "resources": [
    "TIMMessagesSDK/Resources/Bundles/TUIKitFace.bundle",
    "TIMMessagesSDK/Resources/Bundles/TUIKitResource.bundle"
  ],
  "vendored_frameworks": "TIMMessagesSDK/Frameworks/*.framework",
  "source_files": "TIMMessagesSDK/Headers/*.h",
  "dependencies": {
    "TXIMSDK_iOS": [
      "4.9.1"
    ],
    "TXLiteAVSDK_TRTC": [
      "7.8.9519"
    ]
  },
  "resource_bundles": {
    "MessagesSDK": [
      "TIMMessagesSDK/Resources/LocalResources/*"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "VALID_ARCHS": "armv7 arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
