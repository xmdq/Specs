{
  "name": "cglm",
  "version": "0.7.9",
  "summary": "ðŸ“½ Highly Optimized Graphics Math (glm) for C",
  "description": "cglm is math library for graphics programming for C. See the documentation or README for all features.",
  "documentation_url": "http://cglm.readthedocs.io",
  "homepage": "https://github.com/recp/cglm",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Recep Aslantas": "recp@acm.org"
  },
  "source": {
    "git": "https://github.com/recp/cglm.git",
    "tag": "v0.7.9"
  },
  "source_files": [
    "src",
    "include/cglm/**/*.h"
  ],
  "public_header_files": [
    "include",
    "include/cglm/**/*.h"
  ],
  "exclude_files": [
    "src/win/*",
    "src/dllmain.c",
    "src/**/*.h"
  ],
  "preserve_paths": [
    "include",
    "src"
  ],
  "header_mappings_dir": "include",
  "libraries": "m",
  "pod_target_xcconfig": {
    "CLANG_ENABLE_MODULES": "NO",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO",
    "GCC_C_LANGUAGE_STANDARD": "gnu11",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GLM_TESTS_NO_COLORFUL_OUTPUT"
  },
  "platforms": {
    "osx": null,
    "ios": null,
    "tvos": null,
    "watchos": null
  }
}
