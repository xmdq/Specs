{
  "name": "TCUtil",
  "version": "1.0.1",
  "summary": "iOS OC基础库Util",
  "description": "iOS OC基础库Util，目前支持：系统类扩展、文件操作、调试、系统信息、json/xml解析、绘制等方便开发的工具类",
  "homepage": "https://github.com/flatads/tcutil",
  "license": "MIT",
  "authors": {
    "flatincbr": "flatincbr.dev@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/flatads/tcutil.git",
    "tag": "1.0.1"
  },
  "libraries": "c++",
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "AdSupport",
    "Foundation",
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "MobileCoreServices"
  ],
  "public_header_files": "TCUtil/**/*.h",
  "source_files": [
    "TCUtil",
    "TCUtil/**/*.{h,m,mm}"
  ],
  "vendored_frameworks": "TCUtil/**/*.{framework}",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
