{
  "name": "KandyCPaaSMobileSDK",
  "version": "2.11.0",
  "summary": "KandyCPaaSMobileSDK",
  "homepage": "https://github.com/Kandy-IO/kandy-cpaas-ios-sdk",
  "license": {
    "type": "MIT",
    "file": "mit-license.txt"
  },
  "authors": {
    "Name": "cpaassdk@netas.com.tr"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/Kandy-IO/kandy-cpaas-ios-sdk.git",
    "tag": "2.11.0"
  },
  "dependencies": {
    "KandyWebRTC": [

    ]
  },
  "libraries": [
    "c++",
    "icucore",
    "z",
    "sqlite3",
    "stdc++"
  ],
  "ios": {
    "frameworks": [
      "AVFoundation",
      "SystemConfiguration",
      "CFNetwork",
      "Security",
      "OpenGLES",
      "QuartzCore",
      "CoreAudio",
      "CoreMedia",
      "CoreVideo",
      "CoreGraphics",
      "GLKit",
      "VideoToolbox",
      "AudioToolbox",
      "PushKit"
    ]
  },
  "vendored_frameworks": [
    "CPaaSSDK.framework",
    "WebRTC.framework"
  ],
  "preserve_paths": "CPaaSSDK.framework/SubFrameworks/*.framework",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/KandyCPaaSMobileSDK/CPaaSSDK.framework/SubFrameworks\"",
    "OTHER_LDFLAGS": "$(inherited) -framework WebRTC -ObjC",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
