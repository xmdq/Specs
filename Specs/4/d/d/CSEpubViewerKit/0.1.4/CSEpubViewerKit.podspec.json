{
  "name": "CSEpubViewerKit",
  "version": "0.1.4",
  "summary": "A epub reader flutter plugin project.",
  "description": "An open source epub reader plugin written in swift",
  "homepage": "https://github.com/hvmien/EpubViewerKit.git",
  "license": "MIT",
  "authors": {
    "Mien Hoang": "hvmien93@gmail.com"
  },
  "source": {
    "git": "https://github.com/hvmien/EpubViewerKit.git",
    "tag": "0.1.3"
  },
  "swift_versions": "4.2",
  "requires_arc": true,
  "source_files": [
