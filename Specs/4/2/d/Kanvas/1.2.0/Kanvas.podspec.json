{
  "name": "Kanvas",
  "version": "1.2.0",
  "summary": "A custom camera built for iOS.",
  "homepage": "https://github.com/tumblr/kanvas-ios",
  "license": "MPLv2",
  "source": {
    "git": "https://github.com/tumblr/kanvas-ios.git",
    "tag": "1.2.0"
  },
  "authors": {
    "Jimmy Schementi": "jimmys@tumblr.com",
    "Tony Cheng": "tony@getkanvas.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "4.2",
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "GLKit",
    "OpenGLES",
    "UIKit"
