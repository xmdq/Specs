  },
  "subspecs": [
    {
      "name": "Static",
      "dependencies": {
        "YandexMobileAds": [
          "~> 4.0.0"
        ]
      },
      "preserve_paths": [
        "MoPubYandexMobileAdsAdapters.framework",
        "CHANGELOG.md",
        "README.md"
      ],
      "vendored_frameworks": "MoPubYandexMobileAdsAdapters.framework",
      "source_files": "MoPubYandexMobileAdsAdapters.framework/Versions/A/Headers/*.h",
