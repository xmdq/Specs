{
  "name": "ZBJNaviSDK",
  "version": "2.0.5",
  "summary": "百度导航sdk",
  "description": "haha A short description of ZBJNaviSDK.",
  "homepage": "https://lbsyun.baidu.com/index.php?title=ios-navsdk",
  "license": {
    "type": "Copyright",
    "text": "Copyright (c) 2020 BaiduLBS"
  },
  "authors": {
    "BJBJBJ": "719542086@qq.com"
  },
  "source": {
    "http": "https://mapopen-website-wiki.bj.bcebos.com/DEMO/ZBJNaviSDKV2.0.0.zip"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-w",
    "ENABLE_BITCODE": "NO"
  },
  "frameworks": [
    "Accelerate",
    "CallKit",
    "AdSupport",
    "AssetsLibrary",
    "UserNotifications",
    "MediaPlayer",
    "CoreLocation",
    "WebKit"
  ],
  "libraries": [
    "sqlite3.0",
    "c++",
    "iconv"
  ],
  "default_subspecs": [
    "Navi"
  ],
  "subspecs": [
    {
      "name": "Navi",
      "ios": {
        "source_files": "BaiduNaviSDK/inc/*.{h}",
        "vendored_libraries": "BaiduNaviSDK/libbaiduNaviSDK.a",
        "resources": [
          "BaiduNaviSDK/resource/bundle/*.bundle",
          "BaiduNaviSDK/resource/cfg/*.cfg",
          "BaiduNaviSDK/resource/sty/*.sty"
        ]
      }
    },
    {
      "name": "TTS",
      "ios": {
        "dependencies": {
          "ZBJNaviSDK/Navi": [

          ]
        },
        "vendored_libraries": "BaiduNaviSDK/libBNTTSComponentSDK.a"
      }
    }
  ]
}
