{
  "name": "YWWeChatSDK",
  "version": "0.1.6.6",
  "summary": "包装wxapi 1.8.9版本，包含支付功能",
  "homepage": "https://developers.weixin.qq.com/doc/oplatform/Downloads/iOS_Resource.html",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Xinguang": "wechatkit.github.com@kansea.com"
  },
  "source": {
    "git": "http://new-gitlab.yiyaowang.com/yaowang/yw_private_pod.git",
    "tag": "0.1.6.6"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 i386"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source_files": "Vendor/YWWeChatSDK/YWWeChatSDK/**/*.h",
  "vendored_libraries": "Vendor/YWWeChatSDK/YWWeChatSDK/WeChat/*.a",
  "public_header_files": [
    "Vendor/YWWeChatSDK/YWWeChatSDK/*.h",
    "Vendor/YWWeChatSDK/YWWeChatSDK/WeChat/*.h"
  ],
  "frameworks": [
    "SystemConfiguration",
    "Security",
    "CoreTelephony",
    "CoreGraphics",
    "WebKit"
  ],
  "libraries": [
    "sqlite3",
    "z",
    "c++"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -all_load"
  },
  "prepare_command": "rm -rf Vendor/YWWeChatSDK/YWWeChatSDK/Modules\nmkdir Vendor/YWWeChatSDK/YWWeChatSDK/Modules\ntouch Vendor/YWWeChatSDK/YWWeChatSDK/Modules/module.modulemap\ncat <<-EOF > Vendor/YWWeChatSDK/YWWeChatSDK/Modules/module.modulemap\nmodule YWWeChatSDK {\n    header \"YWWeChatSDK.h\"\n\n    export *\n}\nEOF"
}
