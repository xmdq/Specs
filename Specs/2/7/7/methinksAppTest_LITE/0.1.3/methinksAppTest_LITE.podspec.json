    "tag": "0.1.3"
  },
  "platforms": {
    "ios": "10.0"
  },
  "vendored_frameworks": "methinksAppTest_LITE.framework",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
