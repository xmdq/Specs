{
  "name": "Verizon-Ads-VASTController",
  "version": "1.8.0",
  "summary": "Verizon Ads SDK VAST Controller",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.txt"
  },
  "description": "The VAST Controller handles the display of the VAST ad format.",
  "homepage": "https://sdk.verizonmedia.com/",
  "authors": {
    "Verizon Media": "MobileAdsSDK@verizonmedia.com"
  },
  "source": {
    "http": "https://yahoo.bintray.com/maven/com/verizon/ads/ios-vas-vast-controller-cocoapods-artifact/1.8.0/ios-vas-vast-controller-cocoapods-artifact-1.8.0.zip"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "ios": {
    "vendored_frameworks": "VerizonAdsVASTController.framework"
  },
  "libraries": "xml2",
  "static_framework": true,
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "resources": "VerizonAdsVASTControllerResources.bundle",
  "dependencies": {
    "Verizon-Ads-Core": [
      "~> 2.5"
    ],
    "Verizon-Ads-Support": [
      "~> 1.12"
    ],
    "Verizon-Ads-OMSDK": [
      "~> 1.8"
    ]
  }
}
