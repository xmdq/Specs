{
  "name": "PlaybookAccessibilitySnapshot",
  "version": "0.1.0",
  "authors": {
    "jiayi-zhou": "jiayi.zhou98@gmail.com",
    "ra1028": "r.fe51028.r@gmail.com"
  },
  "homepage": "https://github.com/playbook-ui/accessibility-snapshot-ios",
  "documentation_url": "https://playbook-ui.github.io/accessibility-snapshot-ios",
  "summary": "A library for generating snapshot images of components managed by Playbook with accessibility labels.",
  "source": {
    "git": "https://github.com/playbook-ui/accessibility-snapshot-ios.git",
    "tag": "0.1.0"
  },
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "requires_arc": true,
  "swift_versions": "5.3",
  "platforms": {
    "ios": "12.0"
  },
  "ios": {
    "source_files": "Sources/**/*.swift",
    "frameworks": "XCTest"
  },
  "dependencies": {
    "PlaybookSnapshot": [
      "~> 0.2.0"
    ],
    "AccessibilitySnapshot/Core": [
      "~> 0.4.0"
    ]
  },
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO",
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(PLATFORM_DIR)/Developer/Library/Frameworks\""
  },
  "swift_version": "5.3"
}
