  },
  "source": {
    "git": "https://github.com/abbassabeti/PokemonPod.git",
    "tag": "0.0.26"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "SDKPOD/Classes/**/**/*",
  "resources": [
    "SDKPOD/Assets/*.xcdatamodeld",
    "SDKPOD/Assets/Pokemon.xcdatamodeld/*.xcdatamodel"
