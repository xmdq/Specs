{
  "name": "GeoFire",
  "version": "4.1.0",
  "summary": "Realtime location queries with Firebase.",
  "homepage": "https://github.com/firebase/geofire-objc",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Firebase",
  "source": {
    "git": "https://github.com/firebase/geofire-objc.git",
    "tag": "v4.1.0"
  },
  "source_files": "GeoFire/**/*.{h,m}",
  "documentation_url": "https://geofire-ios.firebaseapp.com/docs/",
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "dependencies": {
      "Firebase/Database": [
        "~> 6.0"
      ]
    }
  },
  "frameworks": [
    "CoreLocation",
    "FirebaseDatabase"
  ],
  "requires_arc": true,
  "static_framework": true,
  "subspecs": [
    {
      "name": "Utils",
      "source_files": [
        "GeoFire/**/GFUtils*.[mh]",
        "GeoFire/**/GFGeoQueryBounds*.[mh]",
        "GeoFire/**/GFGeoHashQuery*.[mh]",
        "GeoFire/**/GFGeoHash*.[mh]",
        "GeoFire/**/GFBase32Utils*.[mh]"
      ],
      "frameworks": "CoreLocation"
    }
  ]
}
