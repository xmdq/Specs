{
  "name": "Mixpanel-swift",
  "version": "2.10.1",
  "module_name": "Mixpanel",
  "license": "Apache License, Version 2.0",
  "summary": "Mixpanel tracking library for iOS (Swift)",
  "swift_versions": "5.0",
  "homepage": "https://mixpanel.com",
  "authors": {
    "Mixpanel, Inc": "support@mixpanel.com"
  },
  "source": {
    "git": "https://github.com/mixpanel/mixpanel-swift.git",
    "tag": "v2.10.1"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "3.0"
  },
  "ios": {
    "resources": [
      "Sources/*.{png,xib,storyboard}"
    ],
    "frameworks": [
      "UIKit",
      "Foundation",
      "CoreTelephony"
    ],
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) IOS"
    }
  },
  "default_subspecs": "Complete",
  "tvos": {
    "frameworks": [
      "UIKit",
      "Foundation"
    ],
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) TV_OS TV_AUTO_EVENTS"
    }
  },
  "osx": {
    "frameworks": [
      "Cocoa",
      "Foundation"
    ],
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) MAC_OS"
    }
  },
  "watchos": {
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) WATCH_OS"
    }
  },
  "subspecs": [
    {
      "name": "Complete",
      "ios": {
        "pod_target_xcconfig": {
          "OTHER_SWIFT_FLAGS": "$(inherited) -D DECIDE"
        },
        "source_files": [
          "Sources/*.swift"
        ]
      },
      "tvos": {
