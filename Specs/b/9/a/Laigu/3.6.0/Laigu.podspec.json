      ],
      "xcconfig": {
        "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/Laigu/Laigu-SDK-files\""
      },
      "resources": "Laigu-SDK-files/LGChatViewController/Assets/LGChatViewAsset.bundle"
