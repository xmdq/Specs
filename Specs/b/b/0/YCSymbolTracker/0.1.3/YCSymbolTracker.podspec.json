{
  "name": "YCSymbolTracker",
  "version": "0.1.3",
  "summary": "Track symbols.",
  "homepage": "https://github.com/ryan7cruise/YCSymbolTracker",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Yuchen Peng": "yuchenpeng826@hotmail.com"
  },
  "source": {
    "git": "https://github.com/ryan7cruise/YCSymbolTracker.git",
    "tag": "0.1.3"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "source_files": "YCSymbolTracker/*.{h,m}",
    "preserve_paths": "YCSymbolTracker/symbol_tracker.rb",
    "pod_target_xcconfig": {
      "ENABLE_BITCODE": "NO"
    },
    "user_target_xcconfig": {
      "ENABLE_BITCODE": "NO",
      "OTHER_CFLAGS": "$(inherited) -fsanitize-coverage=func,trace-pc-guard",
      "OTHER_SWIFT_FLAGS": "$(inherited) -sanitize=undefined -sanitize-coverage=func"
    }
  }
}
