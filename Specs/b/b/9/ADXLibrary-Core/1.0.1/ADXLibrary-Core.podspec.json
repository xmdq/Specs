  },
  "frameworks": [
    "Accelerate",
    "AdSupport",
    "AudioToolbox",
    "AVFoundation",
    "CFNetwork",
    "CoreGraphics",
    "CoreMotion",
    "CoreMedia",
    "CoreTelephony",
    "Foundation",
    "GLKit",
    "MobileCoreServices",
    "MediaPlayer",
    "QuartzCore",
    "StoreKit",
    "SystemConfiguration",
    "UIKit",
    "VideoToolbox",
    "WebKit"
