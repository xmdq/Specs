{
  "name": "VHClassSDK",
  "version": "3.6.2",
  "authors": {
    "vhall": "xiaoxiang.wang@vhall.com"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://www.vhall.com",
  "source": {
    "git": "https://github.com/vhall/VHClass_SDK_iOS.git",
    "tag": "3.6.2"
  },
  "summary": "VHClassSDK for IOS",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "frameworks": [
    "AVFoundation",
    "VideoToolbox",
    "OpenAL",
    "CoreMedia",
    "CoreTelephony",
    "OpenGLES",
    "MediaPlayer",
    "AssetsLibrary",
    "QuartzCore",
    "JavaScriptCore",
    "Security"
  ],
  "module_name": "VHClassSDK",
  "resources": [
    "README.md"
  ],
  "vendored_frameworks": "VHClassSDK/framework/*.framework",
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "VALID_ARCHS": "armv7 arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
