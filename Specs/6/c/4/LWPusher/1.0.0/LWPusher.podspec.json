{
  "name": "LWPusher",
  "version": "1.0.0",
  "summary": "A short description of LWPusher.",
  "description": "LWPush，推送组件，两行代码快速集成推送功能，基于信鸽推送，支持打开处理自定义的推送落地页。",
  "homepage": "https://github.com/luowei/LWPush",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "luowei": "luowei@wodedata.com"
  },
  "source": {
    "git": "https://github.com/luowei/LWPush.git"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": [
    "LWPusher/Classes/**/*",
    "LWPusher/XGPush/*.{h,m}"
  ],
  "public_header_files": [
    "LWPusher/Classes/**/*.h",
    "LWPusher/Classes/XGPush/**/*.h"
  ],
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "weak_frameworks": "UserNotifications",
  "vendored_libraries": "LWPusher/Classes/XGPush/libXGPush.a",
  "libraries": [
    "sqlite3",
    "z",
    "XGPush"
  ],
  "static_framework": true
}
