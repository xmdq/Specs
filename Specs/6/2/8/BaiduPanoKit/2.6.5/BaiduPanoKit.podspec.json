  },
  "authors": {
    "baidu pano sdk": "dituapi_01@163.com"
  },
  "source": {
    "http": "https://lbsyun-baidu.cdn.bcebos.com/iossdk/pano/2.6.5/BaiduPanoKitV2.6.5.zip"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "libraries": "c++",
  "default_subspecs": [
    "Pano"
  ],
  "subspecs": [
    {
      "name": "Pano",
      "ios": {
        "vendored_frameworks": "BaiduPanoKit/BaiduPanoSDK.framework",
        "resources": "BaiduPanoKit/BaiduPanoSDK.framework/Resource/*.bundle"
      }
    },
