{
  "name": "zwlHDVodNewTestSDK",
  "version": "0.0.9",
  "summary": "An iOS SDK for CCVod Service",
  "authors": {
    "CCVod": "zhaowl@bokecc.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/zhaowl123/zwlHDVodNewTestSDK",
  "source": {
    "git": "https://github.com/zhaowl123/zwlHDVodNewTestSDK.git",
    "tag": "0.0.9"
  },
  "static_framework": true,
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "vendored_libraries": "include/libCCSDK.a",
  "source_files": "include/*.{h,m}",
  "resources": [
    "include/DWResource.bundle",
    "include/vrlibraw.bundle"
  ],
  "frameworks": "CoreServices",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${SDKROOT}/usr/include/libxml2",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "libraries": "xml2",
  "dependencies": {
    "HDBaseSDK": [
      "~> 1.0.2"
    ]
  },
  "preserve_paths": [
    "include/module.moduleamp",
    "include/DWSDK.h"
  ],
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "$(PODS_ROOT)/zwlHDVodNewTestSDK/include",
      "$(PODS_TARGET_SRCROOT)/zwlHDVodNewTestSDK/include"
    ]
  }
}
