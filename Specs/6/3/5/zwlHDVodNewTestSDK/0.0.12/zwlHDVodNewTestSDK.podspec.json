{
  "name": "zwlHDVodNewTestSDK",
  "version": "0.0.12",
  "summary": "An iOS SDK for CCVod Service",
  "authors": {
    "CCVod": "zhaowl@bokecc.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/zhaowl123/zwlHDVodNewTestSDK",
  "source": {
    "git": "https://github.com/zhaowl123/zwlHDVodNewTestSDK.git",
    "tag": "0.0.12"
  },
  "static_framework": true,
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "vendored_libraries": "include/libnewOCSDK.a",
  "source_files": "include/newOCSDK/*.{h,m}",
  "frameworks": "CoreServices",
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "preserve_paths": [
    "include/Modules/module.moduleamp",
    "include/Modules/newDefine.h"
  ],
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "$(PODS_ROOT)/zwlHDVodNewTestSDK/Module",
      "$(PODS_TARGET_SRCROOT)/zwlHDVodNewTestSDK/Module"
    ]
  }
}
