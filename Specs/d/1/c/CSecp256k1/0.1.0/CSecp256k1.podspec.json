  "swift_versions": [
    "5",
    "5.1",
    "5.2",
    "5.3"
  ],
  "module_name": "CSecp256k1",
  "source_files": [
    "Sources/CSecp256k1/config/*.h",
    "Sources/CSecp256k1/secp256k1/include/*.h",
