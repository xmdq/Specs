    "GCC_WARN_UNUSED_VARIABLE": "NO",
    "GCC_WARN_64_TO_32_BIT_CONVERSION": "NO"
  },
  "testspecs": [
    {
      "name": "CSecp256k1Tests",
      "test_type": "unit",
      "platforms": {
        "ios": "9.0",
        "osx": "10.10",
        "tvos": "9.0"
      },
      "source_files": "Tests/CSecp256k1Tests/**/*.swift"
    }
  ],
  "swift_version": "5.3"
