  "license": {
    "type": "Copyright",
    "text": "Copyright Teads 2021"
  },
  "authors": {
    "Teads": "support-sdk@teads.tv"
  },
  "source": {
    "git": "https://github.com/teads/TeadsSDK-adapter-admob-ios.git",
    "tag": "v4.8.5"
  },
  "platforms": {
    "ios": "9.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "source_files": "TeadsAdMobAdapter/**/*{.swift}",
  "swift_versions": [
    "4.3",
