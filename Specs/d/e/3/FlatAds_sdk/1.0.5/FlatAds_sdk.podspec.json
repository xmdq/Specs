{
  "name": "FlatAds_sdk",
  "version": "1.0.5",
  "summary": "Flat Ads SDK for iOS",
  "description": "Flat Ads SDK for iOS",
  "homepage": "https://github.com/flatads/FlatAdsSDK",
  "license": "MIT",
  "authors": {
    "FlatAds Team": "flatincbr.dev@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/flatads/FlatAdsSDK.git",
    "tag": "1.0.5"
  },
  "libraries": "c++",
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "Foundation",
    "Security",
    "WebKit",
    "SystemConfiguration",
    "CoreTelephony",
    "MobileCoreServices",
    "AppTrackingTransparency",
    "AVFoundation",
    "CoreMedia"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "resource_bundles": {
    "FlatAds_sdk": [
      "FlatAds_sdk/**/*.xcassets"
    ]
  },
  "vendored_frameworks": "FlatAds_sdk/**/*.{framework}",
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "FMDB": [
      "~> 2.6.2"
    ],
    "TCFoundation": [
      "~> 1.0.2"
    ],
    "MJExtension": [
      "~> 3.2.2"
    ],
    "Masonry": [
      "~> 1.1.0"
    ],
    "GoogleAds-IMA-iOS-SDK": [
      "~> 3.14.3"
    ],
    "YYWebImage": [
      "~> 1.0.5"
    ],
    "YYImage/WebP": [
      "~> 1.0.4"
    ]
  }
}
