      "public_header_files": "Sources/BraintreeVenmo/Public/BraintreeVenmo/*.h",
      "dependencies": {
        "Braintree/Core": [

        ],
        "Braintree/PayPalDataCollector": [

        ]
      }
    }
  ],
  "swift_version": "5.1"
}
