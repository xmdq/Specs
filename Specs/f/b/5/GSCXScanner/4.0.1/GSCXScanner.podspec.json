{
  "name": "GSCXScanner",
  "version": "4.0.1",
  "summary": "iOS Accessibility Scanner.",
  "description": "iOS Accessibility scanner framework to catch a11y issues during development.",
  "homepage": "https://github.com/google/GSCXScanner",
  "license": "Apache License 2.0",
  "authors": "j-sid",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/google/GSCXScanner.git",
    "tag": "4.0.1"
  },
  "source_files": "Sources/**/*.{h,m,swift}",
  "resources": [
    "Sources/**/*.{xib}"
  ],
  "resource_bundles": {
