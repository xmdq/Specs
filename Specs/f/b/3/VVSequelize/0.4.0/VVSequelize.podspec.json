{
  "name": "VVSequelize",
  "version": "0.4.0",
  "summary": "ORM model based on SQLite3.",
  "description": "ORM model based on SQLite3.",
  "homepage": "https://github.com/pozi119/VVSequelize",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
