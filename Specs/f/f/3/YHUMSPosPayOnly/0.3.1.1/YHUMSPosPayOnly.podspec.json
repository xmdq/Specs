  },
  "frameworks": [
    "SystemConfiguration",
    "CoreMotion",
    "CFNetwork",
    "CoreTelephony",
    "QuartzCore",
    "CoreText",
    "CoreGraphics",
    "UIKit",
    "Foundation"
  ],
  "libraries": [
    "z",
    "c++",
    "sqlite3"
