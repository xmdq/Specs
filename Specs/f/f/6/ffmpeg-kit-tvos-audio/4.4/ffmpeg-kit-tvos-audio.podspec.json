{
  "name": "ffmpeg-kit-tvos-audio",
  "version": "4.4",
  "summary": "FFmpeg Kit tvOS Audio Static Framework",
  "description": "Includes FFmpeg with lame, libilbc, libvorbis, opencore-amr, opus, shine, soxr, speex, twolame and vo-amrwbenc libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/tvos-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "10.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.4/ffmpeg-kit-audio-4.4-tvos-xcframework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "VideoToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework",
      "lame.xcframework",
      "libilbc.xcframework",
      "libogg.xcframework",
      "libopencore-amrnb.xcframework",
      "libsndfile.xcframework",
      "libvorbis.xcframework",
      "libvorbisenc.xcframework",
      "libvorbisfile.xcframework",
      "opus.xcframework",
      "shine.xcframework",
      "soxr.xcframework",
      "speex.xcframework",
      "twolame.xcframework",
      "vo-amrwbenc.xcframework"
    ]
  }
}
