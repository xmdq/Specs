    "LBXScan/UI": [
      "~> 2.3"
    ],
    "SDAutoLayout": [

    ],
    "HDAlertView": [

    ],
    "MJRefresh": [

    ],
    "IQKeyboardManager": [

    ],
    "SDWebImage": [
      "5.1.1"
    ],
    "MJExtension": [

    ],
    "Masonry": [

    ],
    "TZImagePickerController": [

    ],
    "PPBadgeView": [
      "~> 2.1.0"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "VALID_ARCHS": "armv7 arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
