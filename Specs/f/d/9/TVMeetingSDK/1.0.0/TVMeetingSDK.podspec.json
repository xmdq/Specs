    "loneKiss": "775865767@qq.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/loneKiss/TVMeetingSDK.git",
    "tag": "1.0.0"
  },
  "resources": [
    "TVMeetingSDKReadme.md"
