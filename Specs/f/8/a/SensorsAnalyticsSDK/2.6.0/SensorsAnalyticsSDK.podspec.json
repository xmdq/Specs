        "SensorsAnalyticsSDK/Core": [

        ]
      },
      "source_files": "SensorsAnalyticsSDK/Location/**/*.{h,m}",
      "private_header_files": "SensorsAnalyticsSDK/Location/**/*.h"
    },
    {
      "name": "DeviceOrientation",
