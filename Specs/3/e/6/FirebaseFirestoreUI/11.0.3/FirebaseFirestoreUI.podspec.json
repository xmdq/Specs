{
  "name": "FirebaseFirestoreUI",
  "version": "11.0.3",
  "summary": "Data libraries and UI bindings for Firestore.",
  "homepage": "https://github.com/firebase/FirebaseUI-iOS",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/firebase/FirebaseUI-iOS.git",
    "tag": "v11.0.3"
  },
  "authors": "Firebase",
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "frameworks": "UIKit"
  },
  "requires_arc": true,
  "cocoapods_version": ">= 1.8.0",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
