{
  "name": "Flipper-Fmt",
  "version": "7.1.5",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/fmtlib/fmt",
  "authors": {
    "Pritesh Nandgaonkar": "prit91@fb.com"
  },
  "summary": "{fmt} is an open-source formatting library providing a fast and safe alternative to C stdio and C++ iostreams.",
  "source": {
    "git": "https://github.com/priteshrnandgaonkar/fmt.git",
    "tag": "7.1.4"
  },
  "compiler_flags": "-DFMT_USE_CONSTEXPR=1",
  "public_header_files": "include/**/*.h",
  "source_files": [
    "src/*.cc",
    "include/**/*.h",
    "include/**/*.h"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "10.0"
  },
  "header_dir": "fmt",
  "libraries": "stdc++",
  "exclude_files": [
    "src/fmt.cc",
    "src/fmt.cc"
  ],
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/include/\""
  }
}
