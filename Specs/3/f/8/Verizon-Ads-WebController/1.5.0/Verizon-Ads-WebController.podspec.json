    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "Verizon-Ads-Core": [
      "~> 2.6"
    ],
    "Verizon-Ads-Support": [
      "~> 1.13"
    ],
    "Verizon-Ads-WebView": [
      "~> 1.8"
    ]
