{
  "name": "Verizon-Ads-WebController",
  "version": "1.5.0",
  "summary": "Verizon Ads SDK Web Controller",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.txt"
  },
  "description": "The Web Controller handles the display of web-based ad formats.",
  "homepage": "https://sdk.verizonmedia.com/",
  "authors": {
    "Verizon Media": "MobileAdsSDK@verizonmedia.com"
  },
  "source": {
    "http": "https://artifactory.verizonmedia.com/artifactory/maven/com/verizon/ads/ios-vas-web-controller-cocoapods-artifact/1.5.0/ios-vas-web-controller-cocoapods-artifact-1.5.0.zip"
