{
  "name": "leveldb-pd",
  "version": "1.0.1",
  "summary": "LevelDB is a leveldb library wrapper.",
  "description": "LevelDB is a fast key-value storage library written at Google that provides an ordered mapping from string keys to string values.",
  "homepage": "https://github.com/wangguofeng-live/leveldb-pd",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Onego": "wangguofeng@live.cn"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.8"
  },
  "source": {
    "git": "https://github.com/wangguofeng-live/leveldb-pd.git",
    "tag": "1.0.1"
  },
  "source_files": [
    "include/leveldb/*.{h}",
    "*.{h,m,mm}"
  ],
  "public_header_files": [
    "*.h",
    "include/leveldb/*.h"
  ],
  "libraries": "c++",
  "requires_arc": true,
  "dependencies": {
    "leveldb-library": [

    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/leveldb-pd/include/leveldb"
  }
}
