{
  "name": "MLCardForm",
  "version": "0.9.8",
  "summary": "MLCardForm for iOS",
  "homepage": "https://www.mercadolibre.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "PX Team - Juan Sanzone - Eric Ertl - Esteban Boffa",
  "source": {
    "git": "https://github.com/mercadolibre/card-form-ios",
    "tag": "0.9.8"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "default_subspecs": "Default",
  "static_framework": true,
  "dependencies": {
    "MLUI": [
      "~> 5.0"
    ],
    "AndesUI": [
      "~> 3.0"
    ],
    "MLCardDrawer": [
      "~> 1.0"
    ]
  },
  "subspecs": [
    {
      "name": "Default",
      "source_files": [
        "Source/**/**/**/*.{h,m,swift}"
      ],
      "resources": [
        "Source/UI/**/*.xib"
      ],
      "resource_bundles": {
        "MLCardFormResources": [
          "Source/Resources/*.xcassets",
          "Source/Translations/**/**.{strings}"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
