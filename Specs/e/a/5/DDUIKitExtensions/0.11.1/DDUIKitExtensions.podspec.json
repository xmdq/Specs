{
  "name": "DDUIKitExtensions",
  "version": "0.11.1",
  "summary": "Helpful set of homemade UIKit extensions",
  "homepage": "http://dashdevs.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "dashdevs llc": "hello@dashdevs.com"
  },
  "source": {
    "git": "https://github.com/dashdevs/uikitextensions.git",
    "tag": "0.11.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "UIKitExtensions/*.{h,m,swift}",
  "frameworks": "UIKit",
  "swift_versions": "4.2",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "UIKitExtensions/*.{h,m,swift}"
    },
    {
      "name": "CGSize",
      "source_files": "UIKitExtensions/CGSize+DDExtensions.{swift}"
    },
    {
      "name": "UIColor",
      "source_files": "UIKitExtensions/UIColor+DDExtensions.{h,m}"
    },
    {
      "name": "UIImage",
      "source_files": "UIKitExtensions/UIImage+DDExtensions.{h,m}"
    },
    {
      "name": "UINavigationController",
      "source_files": "UIKitExtensions/UINavigationController+DDExtensions.{h,m}"
    },
    {
      "name": "UITableView",
      "source_files": "UIKitExtensions/UITableView+DDExtensions.{h,m}"
    },
    {
      "name": "UIView",
      "source_files": "UIKitExtensions/UIView+DDExtensions.{h,m}"
    },
    {
      "name": "UIWebView",
      "source_files": "UIKitExtensions/UIWebView+DDExtensions.{h,m}"
    },
    {
      "name": "UIViewController",
      "source_files": "UIKitExtensions/UIViewController+DDExtensions.swift"
    },
    {
      "name": "UITextField",
      "source_files": "UIKitExtensions/UITextField+DDExtensions.{h,m}"
    },
    {
      "name": "UIApplication",
      "source_files": "UIKitExtensions/UIApplication+DDExtensions.{h,m}"
    },
    {
      "name": "UIKit+Sketch",
      "source_files": [
        "UIKitExtensions/CALayer+DDExtensions.swift",
        "UIKitExtensions/NSAttributedString+DDExtensions.swift",
        "UIKitExtensions/UIScreen+DDExtensions.swift"
      ]
    },
    {
      "name": "PaddedLabel",
      "source_files": "UIKitExtensions/PaddedLabel.swift"
    }
  ],
  "swift_version": "4.2"
}
