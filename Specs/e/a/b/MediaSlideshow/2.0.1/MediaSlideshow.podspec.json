    "tag": "2.0.1"
  },
  "swift_versions": [
    "4.0",
    "4.1",
    "4.2",
    "5",
    "5.1",
    "5.2"
  ],
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
