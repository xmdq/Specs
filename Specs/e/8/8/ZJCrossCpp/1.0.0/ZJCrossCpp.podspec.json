{
  "name": "ZJCrossCpp",
  "version": "1.0.0",
  "summary": "C++ cross-platform tool set and basic framework.",
  "description": "C++ cross-platform tool set and basic framework.",
  "homepage": "https://github.com/Eafy/ZJCrossCpp",
  "license": {
    "type": "MIT"
  },
  "authors": "Eafy",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7 arm64 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7 arm64 x86_64"
  },
  "source": {
    "git": "https://github.com/Eafy/ZJCrossCpp.git",
    "tag": "v1.0.0"
  },
  "vendored_libraries": "Libs/iOS/**/*.a",
  "resources": "Libs/iOS/**/*.a",
  "libraries": "c++",
  "dependencies": {
    "ZJLog": [
      "1.1.6"
    ]
  },
  "source_files": [
    "Libs/iOS/**/*.{h,hpp}",
    "Cpp/**/*.{h,hpp,c,cpp}"
  ],
  "public_header_files": [
    "Libs/iOS/**/*.{h,hpp}",
    "Cpp/**/*.{h,hpp}"
  ]
}
