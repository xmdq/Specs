{
  "name": "GJImage",
  "version": "0.1.5",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "An open source iOS framework for GPU-based image and video processing.",
  "homepage": "https://github.com/MuZiLee/GJImage",
  "authors": {
    "MuZiLee": "admin@sanshengit.com"
  },
  "source": {
    "git": "https://github.com/MuZiLee/GJImage.git",
    "tag": "0.1.5"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": [
    "GJImage/*.{h,m}",
    "GJImage/Filters/**/*.{h,m}",
    "GJImage/Outputs/*.{h,m}",
    "GJImage/Pipeline/*.{h,m}",
    "GJImage/Sources/*.{h,m}",
    "GJImageFilters/*.{h,m}"
  ],
  "xcconfig": {
    "CLANG_MODULES_AUTOLINK": "YES",
    "OTHER_LDFLAGS": "-ObjC",
    "ENABLE_BITCODE": "NO"
  },
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "UIKit",
    "OpenGLES",
    "CoreMedia",
    "CoreVideo",
    "QuartzCore",
    "AVFoundation"
  ]
}
