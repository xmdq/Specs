  "subspecs": [
    {
      "name": "Static",
      "dependencies": {
        "YandexMobileAds": [
          "~> 4.2.0"
        ]
      },
      "preserve_paths": [
        "FacebookYandexMobileAdsAdapters.framework",
        "CHANGELOG.md",
        "README.md"
      ],
      "vendored_frameworks": "FacebookYandexMobileAdsAdapters.framework",
      "source_files": "FacebookYandexMobileAdsAdapters.framework/Versions/A/Headers/*.h",
