{
  "name": "TangramX",
  "version": "0.0.3",
  "authors": {
    "lijingcheng": "bj_lijingcheng@163.com"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/lijingcheng/Tangram",
  "source": {
    "git": "https://github.com/lijingcheng/Tangram.git",
    "tag": "0.0.3"
  },
  "summary": "Too big, Too strong, Too fast, Too good!!!",
  "static_framework": true,
  "swift_versions": "5.2",
  "platforms": {
    "ios": "10.0"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "source_files": [
    "R.generated.swift",
    "Tangram/Base/*.swift",
    "Tangram/Extension/**/*.swift",
    "Tangram/Service/*.swift",
    "Tangram/Component/**/*.swift"
  ],
  "resource_bundles": {
    "Tangram": [
      "Tangram/Assets.xcassets"
    ]
  },
  "dependencies": {
    "Alamofire": [
      "5.4.1"
    ],
    "Kingfisher": [
      "5.15.8"
    ],
    "SnapKit": [
      "5.0.1"
    ],
    "R.swift": [
      "5.3.1"
    ],
    "RxSwift": [
      "6.0.0-rc.2"
    ],
    "RxCocoa": [
      "6.0.0-rc.2"
    ],
    "SwiftLint": [
      "0.42.0"
    ]
  },
  "configuration_pod_whitelist": {
    "SwiftLint": [
      "debug"
    ]
  },
  "swift_version": "5.2"
}
