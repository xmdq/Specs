{
  "name": "IronSourceSmaatoAdapter",
  "version": "4.3.0.1",
  "summary": "Smaato Adapter",
  "description": "Use this adapter to show Smaato ads",
  "homepage": "http://www.ironsrc.com/",
  "license": {
    "type": "Commercial",
    "text": "https://platform.ironsrc.com/partners/terms-and-conditions-new-user"
  },
  "authors": {
    "IronSource": "http://www.ironsrc.com/contact/"
  },
  "source": {
    "http": "https://dl.bintray.com/ironsource-mobile/ios-adapters/ISSmaatoAdapter4.3.0.zip"
  },
  "source_files": "ISSmaatoAdapter/ISSmaatoAdapter.framework/**/*.{h,m}",
