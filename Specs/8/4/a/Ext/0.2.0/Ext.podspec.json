{
  "name": "Ext",
  "version": "0.2.0",
  "summary": "Some useful Swift extensions.",
  "description": "Some useful extensions for Swift.",
  "homepage": "https://github.com/naijoug/Ext",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "naijoug": "naijoug@126.com"
  },
  "source": {
    "git": "https://github.com/naijoug/Ext.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "source_files": "Sources",
  "default_subspecs": [
    "Extension",
    "UI",
    "Feature"
  ],
  "subspecs": [
    {
      "name": "Extension",
      "source_files": [
        "Sources/*.swift",
        "Sources/Extension/**/*"
      ],
      "frameworks": [
        "UIKit",
        "AVKit"
      ]
    },
    {
      "name": "UI",
      "source_files": "Sources/UI/**/*",
      "frameworks": [
        "UIKit",
        "WebKit"
      ],
      "dependencies": {
        "Ext/Extension": [

        ]
      }
    },
    {
      "name": "Feature",
      "source_files": "Sources/Feature/**/*",
      "dependencies": {
        "Ext/Extension": [

        ],
        "Ext/UI": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
