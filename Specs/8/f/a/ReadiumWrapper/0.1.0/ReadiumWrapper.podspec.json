{
  "name": "ReadiumWrapper",
  "version": "0.1.0",
  "summary": "A simple wrapper for Readium",
  "homepage": "https://github.com/bmcalister/readium-wrapper",
  "license": "BSD 3-Clause License",
  "authors": {
    "Brian Mc Alister": "brian@mca.io"
  },
  "source": {
    "git": "https://github.com/bmcalister/readium-wrapper.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": [
    "ReadiumWrapper/**/*.{swift, plist, .h, .m}"
  ],
  "exclude_files": [
    "**/Info*.plist"
  ],
  "resources": [
    "ReadiumWrapper/**/*.{png,jpeg,jpg,storyboard,xib,xcassets,json}"
  ],
  "swift_versions": "5",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "preserve_paths": "Frameworks/Build/iOS/*.framework",
  "vendored_frameworks": [
    "Frameworks/Build/iOS/CryptoSwift.framework",
    "Frameworks/Build/iOS/Fuzi.framework",
    "Frameworks/Build/iOS/GCDWebServer.framework",
    "Frameworks/Build/iOS/Kingfisher.framework",
    "Frameworks/Build/iOS/KingfisherSwiftUI.framework",
    "Frameworks/Build/iOS/MBProgressHUD.framework",
    "Frameworks/Build/iOS/Minizip.framework",
    "Frameworks/Build/iOS/R2Navigator.framework",
    "Frameworks/Build/iOS/R2Shared.framework",
    "Frameworks/Build/iOS/R2Streamer.framework",
    "Frameworks/Build/iOS/ReadiumOPDS.framework",
    "Frameworks/Build/iOS/SQLite.framework",
    "Frameworks/Build/iOS/SwiftSoup.framework",
    "Frameworks/Build/iOS/ZIPFoundation.framework"
  ],
  "ios": {
    "frameworks": [
      "CoreServices",
      "SystemConfiguration"
    ]
  },
  "swift_version": "5"
}
