{
  "name": "Verizon-Ads-InterstitialWebAdapter",
  "version": "1.7.0",
  "summary": "Verizon Ads SDK Interstitial Web Adapter",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.txt"
  },
  "description": "The Interstitial Web Adapter connects the Interstitial Placement to the Web Controller. It enables the display of web ad formats on apps that are using the Interstitial Placement.",
  "homepage": "https://sdk.verizonmedia.com/",
  "authors": {
    "Verizon Media": "MobileAdsSDK@verizonmedia.com"
  },
  "source": {
    "http": "https://artifactory.verizonmedia.com/artifactory/maven/com/verizon/ads/ios-vas-interstitial-web-adapter-cocoapods-artifact/1.7.0/ios-vas-interstitial-web-adapter-cocoapods-artifact-1.7.0.zip"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "ios": {
    "vendored_frameworks": "VerizonAdsInterstitialWebAdapter.framework"
  },
  "static_framework": true,
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "Verizon-Ads-Core": [
      "~> 2.6"
    ],
    "Verizon-Ads-Support": [
      "~> 1.13"
    ],
    "Verizon-Ads-InterstitialPlacement": [
      "~> 1.7"
    ],
    "Verizon-Ads-WebController": [
      "~> 1.5"
    ],
    "Verizon-Ads-WebView": [
      "~> 1.8"
    ]
  }
}
