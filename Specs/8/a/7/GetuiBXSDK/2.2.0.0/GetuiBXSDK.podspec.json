        "resolv",
        "sqlite3",
        "xml2",
        "z"
      ],
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-lObjC",
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
