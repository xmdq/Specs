{
  "name": "AptoUISDK",
  "version": "2.8.1",
  "summary": "The Apto UI platform iOS SDK.",
  "description": "Apto iOS UI SDK provides a UI flow that allows to easily integrate the platform in your app.",
  "homepage": "https://github.com/AptoPayments/apto-ui-sdk-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ivan Oliver": "ivan@aptopayments.com",
    "Takeichi Kanzaki": "takeichi@aptopayments.com"
  },
  "source": {
    "git": "https://github.com/AptoPayments/apto-ui-sdk-ios.git",
    "tag": "2.8.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "module_name": "AptoUISDK",
  "source_files": [
    "Pod/Classes/ui/**/*.swift"
  ],
  "resources": [
    "Pod/Assets/*.png",
    "Pod/Assets/*.css",
    "Pod/Localization/*.lproj",
    "Pod/Assets/*.xcassets",
    "Pod/Fonts/*.ttf"
  ],
  "frameworks": [
    "UIKit",
    "CoreLocation",
    "Accelerate",
    "AudioToolbox",
    "AVFoundation",
    "CoreGraphics",
    "CoreMedia",
    "CoreVideo",
    "Foundation",
    "MobileCoreServices",
    "OpenGLES",
    "QuartzCore",
    "Security",
    "LocalAuthentication",
    "CallKit"
  ],
  "dependencies": {
    "AptoSDK": [
      "3.0.1"
    ],
    "AptoPCI": [
      "2.0.1"
    ],
    "SnapKit": [
      "~> 5.0"
    ],
    "Bond": [
      "~> 7.6"
    ],
    "GoogleKit": [
      "~> 0.3"
    ],
    "PhoneNumberKit": [
      "~> 3.2"
    ],
    "TTTAttributedLabel": [
      "~> 2.0"
    ],
    "TrustKit": [
      "~> 1.6.0"
    ],
    "Down": [
      "~> 0.8.0"
    ],
    "PullToRefreshKit": [
      "~> 0.8"
    ],
    "TwilioVoice": [
      "~> 3.0"
    ]
  },
  "swift_version": "5.0"
}
