{
  "name": "CloudTalk",
  "version": "1.0.4",
  "license": "MIT",
  "summary": "Cloud Talk on iOS",
  "homepage": "https://github.com/DevilLGD/CloudTalk",
  "authors": {
    "liaogd": "285282227@qq.com"
  },
  "source": {
    "git": "https://github.com/DevilLGD/CloudTalk.git",
    "tag": "1.0.4"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": [
    "5.1",
    "5.2",
    "5.3"
  ],
  "default_subspecs": "Private",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "CloudTalk/Core/**/*.{h,m,swift}",
        "CloudTalk/CloudTalkHeader.h"
      ]
    },
    {
      "name": "Private",
      "source_files": "CloudTalk/Private/*.{h,m,swift}",
      "private_header_files": "CloudTalk/Private/*.h"
    },
    {
      "name": "Util",
      "source_files": "CloudTalk/Util/*.{h,m,swift}",
      "subspecs": [
        {
          "name": "SubUtil",
          "source_files": "CloudTalk/Util/SubUtil/*.{h,m,swift}"
        }
      ]
    }
  ],
  "swift_version": "5.3"
}
