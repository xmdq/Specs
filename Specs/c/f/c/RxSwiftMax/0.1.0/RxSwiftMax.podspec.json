{
  "name": "RxSwiftMax",
  "version": "0.1.0",
  "summary": "RxSwift 升级版",
  "description": "RxSwift 升级版",
  "homepage": "https://github.com/Boy-Rong/RxSwiftMax",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
