    "rongheng": "rongheng.rh@gmail.com"
  },
  "source": {
    "git": "https://github.com/Boy-Rong/RxSwiftMax.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "default_subspecs": [
