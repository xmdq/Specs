  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Maximilian Alexander": "max@ditto.live",
    "Adam Fish": "adam@ditto.live",
    "Hamilton Chapman": "hamilton@ditto.live"
  },
  "dependencies": {
    "DittoObjC": [
      "=1.0.1"
    ]
  },
  "swift_versions": [
    "5.2",
    "5.3"
  ],
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "http": "https://software.ditto.live/cocoa/DittoSwift/1.0.1/dist/DittoSwift-dist.tar.gz"
  },
  "vendored_frameworks": "DittoSwift.xcframework",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
