  "homepage": "https://github.com/Reloadly/reloadly-sdk-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "4taras4": "4taras4@gmail.com"
  },
  "source": {
    "git": "https://github.com/Reloadly/reloadly-sdk-ios.git",
    "tag": "1.0.0"
  },
  "swift_versions": [
    "4.2",
    "5.0"
  ],
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "ReloadlySDK/Classes/**/*",
  "default_subspecs": "Airtime",
