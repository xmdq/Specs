{
  "name": "ConnectAd_ObjC",
  "version": "1.1.4",
  "summary": "ConnectAd_ObjC for iOS.",
  "description": "This pod is used for integrating ConnectAd_ObjC in Objective-C iOS projects.",
  "homepage": "https://github.com/connectedinteractive/connectedadsdk_objc",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "connsdkimpl": "sdkimpl@gmail.com"
  },
  "source": {
    "git": "https://github.com/connectedinteractive/connectedadsdk_objc.git",
    "tag": "1.1.4"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "ConnectAd_ObjC/**/*.{h,m}",
  "exclude_files": "ConnectAd_ObjC/**/*.plist",
  "resource_bundles": {
    "ConnectAd_ObjC": [
      "ConnectAd_ObjC/Assets/*.png"
    ]
  },
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "static_framework": true,
  "dependencies": {
    "mopub-ios-sdk/Core": "~> 5.17.0",
    "Google-Mobile-Ads-SDK": "~> 8.6.0"
  }
}
