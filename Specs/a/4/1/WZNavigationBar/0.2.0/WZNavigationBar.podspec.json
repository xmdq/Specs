{
  "name": "WZNavigationBar",
  "version": "0.2.0",
  "summary": "A custom navigation bar of UIViewController",
  "homepage": "https://github.com/WZLYiOS/WZNavigationBar",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "LiuSky": "327847390@qq.com"
  },
  "source": {
    "git": "https://github.com/WZLYiOS/WZNavigationBar.git",
    "tag": "0.2.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Source",
  "subspecs": [
    {
      "name": "Source",
      "source_files": "WZNavigationBar/Classes/**/*",
      "resource_bundles": {
        "WZNavigationBar": [
          "WZNavigationBar/Assets/*.xcassets"
        ]
      }
    },
    {
      "name": "Binary",
      "vendored_frameworks": "Carthage/Build/iOS/Static/WZNavigationBar.framework",
      "resource_bundles": {
        "WZNavigationBar": [
          "WZNavigationBar/Assets/*.xcassets"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
