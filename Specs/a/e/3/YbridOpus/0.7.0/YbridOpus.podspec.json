{
  "name": "YbridOpus",
  "version": "0.7.0",
  "summary": "Opus xcframework for iOS and macOS.",
  "description": "XCFramework to use Opus Interactive Audio Codec within Swift source.\nIt runs on iOS devices, iOS simulators and macOS.",
  "homepage": "https://github.com/ybrid/opus-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Florian Nowotny": "Florian.Nowotny@nacamar.de"
  },
  "source": {
    "http": "https://github.com/ybrid/opus-swift/releases/download/0.7.0/YbridOpus.xcframework.zip"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.10"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": "YbridOpus",
  "vendored_frameworks": "YbridOpus.xcframework"
}
