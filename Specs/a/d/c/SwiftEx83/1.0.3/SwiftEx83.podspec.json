{
  "name": "SwiftEx83",
  "version": "1.0.3",
  "summary": "Contains Foundation and UIKit extensions that are very helpful in every project.",
  "homepage": "https://gitlab.com/seriyvolk83/SwiftEx",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "seriyvolk83": "volk@frgroup.ru"
  },
  "requires_arc": true,
  "swift_versions": "4.2.1",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://gitlab.com/seriyvolk83/SwiftEx.git",
    "tag": "1.0.3"
  },
  "source_files": "Source/*.swift",
  "default_subspecs": "Data",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/*.swift"
    },
    {
      "name": "Data",
      "dependencies": {
        "SwiftyJSON": [
          "~> 4.0"
        ]
      },
      "source_files": [
        "Source/*.swift",
        "SourceData/*.swift"
      ]
    },
    {
      "name": "UI",
      "dependencies": {
        "SwiftyJSON": [
          "~> 4.0"
        ]
      },
      "source_files": [
        "Source/*.swift",
        "SourceData/*.swift",
        "SourceUI/*.swift"
      ]
    },
    {
      "name": "Api",
      "dependencies": {
        "SwiftyJSON": [
          "~> 4.0"
        ]
      },
      "source_files": [
        "Source/*.swift",
        "SourceData/*.swift",
        "SourceApi/*.swift"
      ]
    },
    {
      "name": "Int",
      "dependencies": {
        "RxAlamofire": [
          "~> 5.0.0"
        ],
        "SwiftyJSON": [
          "~> 4.0"
        ],
        "NSObject+Rx": [

        ]
      },
      "source_files": [
        "Source/*.swift",
        "SourceData/*.swift",
        "SourceUI/*.swift",
        "SourceApi/*.swift",
        "SourceInt/*.swift"
      ]
    }
  ],
  "swift_version": "4.2.1"
}
