{
  "name": "AudioburstSDK",
  "version": "0.0.2",
  "summary": "Official SDK from Audioburst",
  "homepage": "https://github.com/audioburst-labs/AudioburstSDK-iOS",
  "license": {
    "type": "Custom"
  },
  "authors": {
    "Audioburst": "alex.kobylak@audioburst.com"
  },
  "source": {
    "git": "https://github.com/audioburst-labs/AudioburstSDK-iOS.git",
    "tag": "0.0.2"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "source_files": "*/*.swift",
  "dependencies": {
    "AudioburstMobileLibrary": [
      "~> 0.0.21"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "AudioburstPlayerCore",
      "source_files": [
        "PlayerCore/*.swift",
        "Shared/*/*.swift",
        "Shared/*.swift"
      ]
    }
  ],
  "swift_version": "5.0"
}
