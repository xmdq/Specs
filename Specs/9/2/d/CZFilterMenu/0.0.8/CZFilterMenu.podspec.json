{
  "name": "CZFilterMenu",
  "version": "0.0.8",
  "requires_arc": true,
  "platforms": {
    "ios": "10.0"
  },
  "summary": "模仿贝壳找房下拉菜单",
  "description": "模仿贝壳找房下拉菜单,支持一、二、三级列表,自定义输入等",
  "homepage": "https://github.com/JQZhangC/CZFilterMenuView",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "CZ": "cooper_jx@126.com"
  },
  "source": {
    "git": "https://github.com/JQZhangC/CZFilterMenuView.git",
    "tag": "0.0.8"
  },
  "source_files": [
    "CZFilterMenu",
    "CZFilterMenuView/CZFilterMenu/*.{h,m}"
  ],
  "resource_bundles": {
    "CZFilterMenuResource": [
      "CZFilterMenuView/CZFilterMenu/View/*.xib",
      "CZFilterMenuView/CZFilterMenu/*.xcassets"
    ]
  },
  "ios": {
    "frameworks": [
      "Foundation",
      "UIKit"
    ]
  },
  "subspecs": [
    {
      "name": "Helper",
      "source_files": "CZFilterMenuView/CZFilterMenu/Helper/*.{h,m}"
    },
    {
      "name": "View",
      "source_files": "CZFilterMenuView/CZFilterMenu/View/*.{h,m}"
    }
  ]
}
