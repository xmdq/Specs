{
  "name": "Folly-GTest",
  "version": "1.10.6",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/google/googletest.git",
  "authors": {
    "Pritesh Nandgaonkar": "prit91@fb.com"
  },
  "summary": "GTest is googles c++ testing framework",
  "module_name": "gtest",
  "source": {
    "git": "https://github.com/google/googletest.git",
    "tag": "release-1.10.0"
  },
  "source_files": [
    "googletest/include/gtest/*.h",
    "googletest/include/gtest/internal/*.h",
    "googletest/include/gtest/internal/custom/*.h",
    "googletest/src/**/*.h"
  ],
  "preserve_paths": "googletest/include/gtest/**",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10"
  },
  "description": "Just the headers of GTest, a c++ testing framework",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/googletest/include/**\" \"$(PODS_TARGET_SRCROOT)/googletest/src/**\""
  }
}
