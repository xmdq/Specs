{
  "name": "ffmpeg-kit-tvos-video",
  "version": "4.4.LTS",
  "summary": "FFmpeg Kit tvOS Video Static Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, kvazaar, libass, libtheora, libvpx, snappy and libwebp libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.4.LTS/ffmpeg-kit-video-4.4.LTS-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "dav1d.framework",
      "expat.framework",
      "fontconfig.framework",
      "freetype.framework",
      "fribidi.framework",
      "giflib.framework",
      "harfbuzz.framework",
      "jpeg.framework",
      "kvazaar.framework",
      "libass.framework",
      "libogg.framework",
      "libpng.framework",
      "libtheora.framework",
      "libtheoradec.framework",
      "libtheoraenc.framework",
      "libvorbis.framework",
      "libvorbisenc.framework",
      "libvorbisfile.framework",
      "libvpx.framework",
      "libwebp.framework",
      "libwebpmux.framework",
      "libwebpdemux.framework",
      "snappy.framework",
      "tiff.framework"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  }
}
