{
  "name": "Natrium",
  "version": "8.0.3",
  "platforms": {
    "ios": "10.0",
    "osx": "10.10"
  },
  "summary": "An advanced pre-build swift script to alter your project per environment and build configuration.",
  "authors": {
    "Bas van Kuijck": "bas@e-sites.nl"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/e-sites/Natrium",
  "source": {
    "git": "https://github.com/e-sites/Natrium.git",
    "tag": "8.0.3"
  },
  "preserve_paths": [
    "natrium"
  ],
  "public_header_files": [
    "Natrium/Sources/Natrium.h"
  ],
  "source_files": [
    "Natrium/Sources/Natrium.h",
    "Natrium/Sources/Natrium.swift"
  ],
  "requires_arc": true,
  "frameworks": "Foundation",
  "swift_versions": [
    "4.2",
    "5.0",
    "5.3"
  ],
  "prepare_command": "cp -f ./Natrium/Sources/Natrium.swift ./Natrium/Natrium.swift\nchmod 7777 ./Natrium/Natrium.swift",
  "swift_version": "5.3"
}
